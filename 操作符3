#include <stdio.h>

int main()
{
	int a = 21;
	int c;

	c = a;
	printf("%d\n", c);//结果为21

	c += a;
	printf("%d\n", c);//结果为21+21=42

	c -= a;
	printf("%d\n", c);//结果为42-21=21

	c *= a;
	printf("%d\n", c);//结果为21*21=441

	c /= a;
	printf("%d\n", c);//结果为441/21=21

	c = 200;
	c %= a;
	printf("%d\n", c);//结果为200%21=11

	c <<= 2;//11转换成二进制为1011，c<<2为101100，转换为十进制是44
	printf("%d\n", c);//结果为44

	c >>= 2;//44转换为二进制为101100，c>>2为1011，转换为十进制是11
	printf("%d\n", c);//结果为11

	c &= 2;//11转换为二进制为1011，c&2为0010，转换为十进制是2
	printf("%d\n", c);//结果为2

	c |= 2;//2转换为二进制是10，c|2为10，转换为十进制是2
	printf("%d\n", c);//结果为2

	c ^= 2;//2转换为二进制是10，c^2为00，转换为十进制是0
	printf("%d\n", c);//结果为0

	return 0;
}
